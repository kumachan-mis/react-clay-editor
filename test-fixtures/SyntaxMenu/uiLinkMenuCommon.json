{
  "note": [
    "There are some categories in these tests:",
    "- menuitem: bracket hashtag tagged-link-button tagged-link-dropdown",
    "- selection: no-selection single-line-selection multi-lines-selection",
    "  - no-selection: empty-line in-link in-normal other",
    "  - single-line-selection: in-link in-normal other",
    "  - multi-lines-selection"
  ],
  "common": {
    "options": {
      "bracketLinkProps": {
        "suggestions": ["blueberries", "cantaloupe", "breadfruit"]
      },
      "hashtagProps": {
        "suggestions": ["cherries", "durian", "coconut meat"]
      },
      "taggedLinkPropsMap": {
        "tag": {
          "suggestions": ["feijoa", "elderberries", "figs"]
        },
        "@": {
          "suggestions": ["feIjOa", "eldErberrIes", "fiGs"]
        }
      }
    }
  },
