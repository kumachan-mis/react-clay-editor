{
  "note": [
    "There are some categories in these tests:",
    "- section menu",
    "  - menuitem: button normal larger largest",
    "  - selection: no-selection single-line-selection multi-lines-selection",
    "    - no-selection: empty-line normal-line section-line section-like-line other",
    "    - single-line-selection: normal-line section-line section-like-line other",
    "    - multi-lines-selection",
    "- itemization menu",
    "  - menuitem: button indent outdent",
    "  - selection: no-selection selection",
    "    - no-selection: empty-line no-itemized-line itemized-line nested-itemized-line",
    "    - selection: no-itemized-line has-itemized-line",
    "- decoration menu",
    "  - menuitem: bold italic underline",
    "  - selection: no-selection single-line-selection multi-lines-selection",
    "    - no-selection: empty-line in-decoration in-lacked in-normal other",
    "    - single-line-selection: empty-line in-decoration-all in-decoration-left in-decoration-mid in-decoration-right",
    "                             in-lacked in-lacked-with-decoration-left in-lacked-with-decoration-right",
    "                             in-normal in-normal-with-decoration-left in-normal-with-decoration-right other",
    "    - multi-lines-selection"
  ],
