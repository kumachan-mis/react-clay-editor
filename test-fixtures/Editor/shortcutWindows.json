{
  "common": {
    "typingAlias": {
      "{THREE_WORDS_SENT_TOP}": [
        "three words,sentence.{enter}three words,sentence.{enter}three words,sentence.",
        "{arrowup}{arrowleft}{arrowleft}"
      ],
      "{THREE_WORDS_SENT_BOTTOM}": [
        "three words,sentence.{enter}three words,sentence.{enter}three words,sentence.",
        "{arrowup}{home}"
      ]
    }
  },
  "testCases": [
    {
      "name": "PRETEST: typing alias {THREE_WORDS_SENT_TOP}",
      "inputTyping": ["{THREE_WORDS_SENT_TOP}", "0"],
      "expectedLines": ["three words,sentence.", "three words,sentenc0e.", "three words,sentence."]
    },
    {
      "name": "PRETEST: typing alias {THREE_WORDS_SENT_BOTTOM}",
      "inputTyping": ["{THREE_WORDS_SENT_BOTTOM}", "0"],
      "expectedLines": ["three words,sentence.", "0three words,sentence.", "three words,sentence."]
    },
    {
      "name": "select all (ctrl+a)",
      "inputTyping": ["Hello{enter}World{ctrl}a{/ctrl}{backspace}."],
      "expectedLines": ["."]
    },
    {
      "name": "undo insert (ctrl+z)",
      "inputTyping": ["abc{arrowright}{arrowright}{arrowright}{ctrl}z{/ctrl}d"],
      "expectedLines": ["abd"]
    },
    {
      "name": "undo delete (ctrl+z)",
      "inputTyping": ["abc{backspace}{arrowright}{arrowright}", "{ctrl}z{/ctrl}d"],
      "expectedLines": ["abcd"]
    },
    {
      "name": "redo insert (ctrl+shift+z)",
      "inputTyping": [
        "{ctrl}{shift}z{/shift}{/ctrl}",
        "abc{ctrl}zzz{/ctrl}{arrowright}{arrowright}",
        "{ctrl}{shift}zz{/shift}{/ctrl}d"
      ],
      "expectedLines": ["abd"]
    },
    {
      "name": "redo delete (ctrl+shift+z)",
      "inputTyping": ["abc{backspace}{ctrl}z{/ctrl}{arrowright}{arrowright}", "{ctrl}{shift}z{/shift}{/ctrl}d"],
      "expectedLines": ["abd"]
    },
    {
      "name": "redo (ctrl+y)",
      "inputTyping": ["{ctrl}y{/ctrl}", "abc{ctrl}zzz{/ctrl}{arrowright}{arrowright}", "{ctrl}yy{/ctrl}d"],
      "expectedLines": ["abd"]
    },
    {
      "name": "move word top (ctrl+arrowleft)",
      "inputTyping": [
        "{THREE_WORDS_SENT_TOP}",
        "{ctrl}{arrowleft}{/ctrl}0{arrowleft}",
        "{ctrl}{arrowleft}{/ctrl}1{arrowleft}",
        "{ctrl}{arrowleft}{/ctrl}2{arrowleft}"
      ],
      "expectedLines": ["three words,sentence.", "2three 1words,0sentence.", "three words,sentence."]
    },
    {
      "name": "move word bottom (ctrl+arrowright)",
      "inputTyping": [
        "{THREE_WORDS_SENT_BOTTOM}",
        "{ctrl}{arrowright}{/ctrl}0",
        "{ctrl}{arrowright}{/ctrl}1",
        "{ctrl}{arrowright}{/ctrl}2"
      ],
      "expectedLines": ["three words,sentence.", "three0 words1,sentence2.", "three words,sentence."]
    },
    {
      "name": "move line top (home)",
      "inputTyping": ["{THREE_WORDS_SENT_TOP}", "{home}0{home}1"],
      "expectedLines": ["three words,sentence.", "10three words,sentence.", "three words,sentence."]
    },
    {
      "name": "move line bottom (end)",
      "inputTyping": ["{THREE_WORDS_SENT_BOTTOM}", "{end}0{end}1"],
      "expectedLines": ["three words,sentence.", "three words,sentence.01", "three words,sentence."]
    },
    {
      "name": "move text top (ctrl+home)",
      "inputTyping": ["{THREE_WORDS_SENT_TOP}", "{ctrl}{home}{/ctrl}0"],
      "expectedLines": ["0three words,sentence.", "three words,sentence.", "three words,sentence."]
    },
    {
      "name": "move text bottom (ctrl+end)",
      "inputTyping": ["{THREE_WORDS_SENT_BOTTOM}", "{ctrl}{end}{/ctrl}0"],
      "expectedLines": ["three words,sentence.", "three words,sentence.", "three words,sentence.0"]
    }
  ]
}
