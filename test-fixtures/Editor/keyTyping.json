{
  "testCaseGroups": [
    {
      "groupName": "should perform correct state transition in typing normal keys",
      "testCases": [
        {
          "testName": "should perform correct state transition for single key typing",
          "inputState": {
            "cursorCoordinate": {
              "lineIndex": 0,
              "charIndex": 0
            }
          },
          "inputTyping": "a",
          "expectedLines": ["a"],
          "expectedState": {
            "cursorCoordinate": {
              "lineIndex": 0,
              "charIndex": 1
            },
            "textAreaValue": "",
            "historyHead": 0,
            "editActionHistory": [
              {
                "actionType": "insert",
                "coordinate": {
                  "lineIndex": 0,
                  "charIndex": 0
                },
                "text": "a"
              }
            ]
          }
        },
        {
          "testName": "should perform correct state transition for multiple keys typing",
          "inputState": {
            "cursorCoordinate": {
              "lineIndex": 0,
              "charIndex": 0
            }
          },
          "inputTyping": "aBC",
          "expectedLines": ["aBC"],
          "expectedState": {
            "cursorCoordinate": {
              "lineIndex": 0,
              "charIndex": 3
            },
            "textAreaValue": "",
            "historyHead": 2,
            "editActionHistory": [
              {
                "actionType": "insert",
                "coordinate": {
                  "lineIndex": 0,
                  "charIndex": 0
                },
                "text": "a"
              },
              {
                "actionType": "insert",
                "coordinate": {
                  "lineIndex": 0,
                  "charIndex": 1
                },
                "text": "B"
              },
              {
                "actionType": "insert",
                "coordinate": {
                  "lineIndex": 0,
                  "charIndex": 2
                },
                "text": "C"
              }
            ]
          }
        }
      ]
    }
  ]
}
