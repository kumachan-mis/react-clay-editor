{
  "testCases": [
    {
      "name": "basic input",
      "inputTyping": ["abc"],
      "expectedLines": ["abc"]
    },
    {
      "name": "tab",
      "inputTyping": ["Hello\tWorld"],
      "expectedLines": ["Hello\tWorld"]
    },
    {
      "name": "enter",
      "inputTyping": ["Hello{enter}", "World"],
      "expectedLines": ["Hello", "World"]
    },
    {
      "name": "backspace",
      "inputTyping": ["Hell{enter}{backspace}o{enter}", "Wp{backspace}orld"],
      "expectedLines": ["Hello", "World"]
    },
    {
      "name": "delete",
      "inputTyping": ["Hellp{arrowleft}{del}o{enter}", "Q{arrowleft}{arrowleft}{del}{del}{enter}World"],
      "expectedLines": ["Hello", "World"]
    },
    {
      "name": "arrowkeys",
      "inputTyping": [
        "ab{enter}",
        "cd",
        "{arrowleft}0{arrowleft}{arrowleft}1{arrowleft}{arrowleft}{arrowleft}{arrowleft}2",
        "{arrowright}3{arrowright}{arrowright}{arrowright}4{arrowright}{arrowright}{arrowright}{arrowright}5",
        "{arrowup}6{arrowup}7",
        "{arrowdown}8{arrowdown}9"
      ],
      "expectedLines": ["72a3b16", "84c0d59"]
    },
    {
      "name": "parens/braces/brackets auto-fill",
      "inputTyping": ["(parens{arrowright}{{braces{arrowright}[[brackets{arrowright}"],
      "expectedLines": ["(parens){braces}[brackets]"]
    },
    {
      "name": "quotation enter",
      "inputTyping": [">{space}first{space}quotation{enter}", "second{space}quotation"],
      "expectedLines": ["> first quotation", "> second quotation"]
    },
    {
      "name": "quotation backspace",
      "inputTyping": [">{space}q{backspace}{backspace}empty"],
      "expectedLines": ["empty"]
    }
  ]
}
