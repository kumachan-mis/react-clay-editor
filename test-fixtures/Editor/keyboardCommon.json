{
  "testCases": [
    {
      "name": "basic input",
      "inputTyping": ["abc"],
      "expectedLines": ["abc"]
    },
    {
      "name": "tab",
      "inputTyping": ["Hello\tWorld"],
      "expectedLines": ["Hello\tWorld"]
    },
    {
      "name": "enter",
      "inputTyping": ["Hello{enter}", "World"],
      "expectedLines": ["Hello", "World"]
    },
    {
      "name": "backspace",
      "inputTyping": ["Hell{enter}{backspace}o{enter}", "Wp{backspace}orld"],
      "expectedLines": ["Hello", "World"]
    },
    {
      "name": "backspace with selection",
      "inputTyping": ["Hello{enter}World", "{shift}{home}{/shift}{backspace}"],
      "expectedLines": ["Hello", ""]
    },
    {
      "name": "delete",
      "inputTyping": ["Hellp{arrowleft}{del}o{enter}", "Q{arrowleft}{arrowleft}{del}{del}{enter}World"],
      "expectedLines": ["Hello", "World"]
    },
    {
      "name": "delete with selection",
      "inputTyping": ["Hello{enter}World", "{shift}{home}{/shift}{del}"],
      "expectedLines": ["Hello", ""]
    },
    {
      "name": "arrowleft",
      "inputTyping": [
        "ab{enter}cd",
        "{arrowleft}1{arrowleft}{arrowleft}{arrowleft}2{arrowleft}{arrowleft}{arrowleft}{arrowleft}3"
      ],
      "expectedLines": ["3ab2", "c1d"]
    },
    {
      "name": "arrowright",
      "inputTyping": [
        "ab{enter}cd{arrowleft}{arrowleft}{arrowleft}{arrowleft}{arrowleft}",
        "{arrowright}1{arrowright}{arrowright}2{arrowright}{arrowright}{arrowright}3"
      ],
      "expectedLines": ["a1b", "2cd3"]
    },
    {
      "name": "arrowup",
      "inputTyping": ["abcde{enter}fg{enter}hijkl", "{arrowup}1{arrowup}2{arrowup}{arrowup}3"],
      "expectedLines": ["3abc2de", "fg1", "hijkl"]
    },
    {
      "name": "arrowdown",
      "inputTyping": [
        "abcde{enter}fg{enter}hijkl{arrowup}{arrowleft}{arrowleft}{arrowleft}",
        "{arrowdown}1{arrowdown}2{arrowdown}{arrowdown}3"
      ],
      "expectedLines": ["abcde", "fg1", "hij2kl3"]
    },
    {
      "name": "parens/braces/brackets auto-fill",
      "inputTyping": ["({arrowright}{{{arrowright}[[{arrowright}"],
      "expectedLines": ["(){}[]"]
    },
    {
      "name": "parens/braces/brackets auto-fill with single-line selection",
      "inputTyping": [
        "Hello{space}Parens{home}{shift}{end}{/shift}({arrowright}.{enter}",
        "Hello{space}Braces{home}{shift}{end}{/shift}{{{arrowright}.{enter}",
        "Hello{space}Brackets{home}{shift}{end}{/shift}[[{arrowright}."
      ],
      "expectedLines": ["(Hello Parens).", "{Hello Braces}.", "[Hello Brackets]."]
    },
    {
      "name": "parens/braces/brackets auto-fill with multi-lines selection",
      "inputTyping": [
        "Hello{enter}Parens{arrowup}{home}{shift}{arrowdown}{end}{/shift}({arrowright}.{enter}",
        "Hello{enter}Braces{arrowup}{home}{shift}{arrowdown}{end}{/shift}{{{arrowright}.{enter}",
        "Hello{enter}Brackets{arrowup}{home}{shift}{arrowdown}{end}{/shift}[[{arrowright}."
      ],
      "expectedLines": ["(Hello", "Parens).", "{Hello", "Braces}.", "[Hello", "Brackets]."]
    },
    {
      "name": "parens/braces/brackets auto-del",
      "inputTyping": [
        "({arrowright}{{{arrowright}[[{arrowright}",
        "{arrowleft}{backspace}{arrowleft}{backspace}{arrowleft}{backspace}."
      ],
      "expectedLines": ["."]
    },
    {
      "name": "quotation enter",
      "inputTyping": [">{space}first{space}quotation{enter}", "second{space}quotation"],
      "expectedLines": ["> first quotation", "> second quotation"]
    },
    {
      "name": "quotation backspace",
      "inputTyping": [">{space}q{backspace}{backspace}{backspace}."],
      "expectedLines": ["."]
    }
  ]
}
