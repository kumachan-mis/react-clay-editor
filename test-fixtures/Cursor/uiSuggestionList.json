{
  "common": {
    "options": {
      "textProps": {
        "suggestions": ["apple", "banana", "acerola"]
      },
      "bracketLinkProps": {
        "suggestions": ["blueberries", "cantaloupe", "breadfruit"]
      },
      "hashtagProps": {
        "suggestions": ["cherries", "durian", "coconut meat"]
      },
      "taggedLinkPropsMap": {
        "tag": {
          "suggestions": ["feijoa", "elderberries", "figs"]
        }
      }
    }
  },
  "testCases": [
    {
      "name": "text suggestion",
      "inputTypingBefore": ["a"],
      "inputClickIndex": 1,
      "inputTypingAfter": ["."],
      "expectedHeader": "Text Suggestion",
      "expectedLines": ["acerola."]
    },
    {
      "name": "bracket link suggestion",
      "inputTypingBefore": ["[[c"],
      "inputClickIndex": 0,
      "inputTypingAfter": ["{arrowright}."],
      "expectedHeader": "Bracket Link Suggestion",
      "expectedLines": ["[cantaloupe]."]
    },
    {
      "name": "hash tag suggestion",
      "inputTypingBefore": ["#"],
      "inputClickIndex": 2,
      "inputTypingAfter": ["."],
      "expectedHeader": "Hashtag Suggestion",
      "expectedLines": ["#coconut_meat ."]
    },
    {
      "name": "tagged link suggestion",
      "inputTypingBefore": ["[[tag:"],
      "inputClickIndex": 0,
      "inputTypingAfter": ["{arrowright}."],
      "expectedHeader": "Tagged Link Suggestion",
      "expectedLines": ["[tag: feijoa]."]
    }
  ]
}
