    },
    {
      "name": "hash tag suggestion",
      "inputTyping": ["#"],
      "inputClickIndex": 2,
      "expectedHeader": "HashTag Suggestion",
      "expectedLines": ["#coconut_meat "]
    },
    {
      "name": "tagged link suggestion",
      "inputTyping": ["[[tag:"],
      "inputClickIndex": 0,
      "expectedHeader": "Tagged Link Suggestion",
      "expectedLines": ["[tag: feijoa]"]
    }
