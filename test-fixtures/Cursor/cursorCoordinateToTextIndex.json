{
  "testCases": [
    {
      "name": "line zero",
      "inputLines": ["123456789/123456789", "123456789", "123456789/123456789"],
      "inputCursorCoordinate": {
        "lineIndex": 0,
        "charIndex": 7
      },
      "expectedIndex": 7
    },
    {
      "name": "line non-zero",
      "inputLines": ["123456789/123456789", "123456789", "123456789/123456789"],
      "inputCursorCoordinate": {
        "lineIndex": 2,
        "charIndex": 3
      },
      "expectedIndex": 33
    },
    {
      "name": "empty text",
      "inputLines": [],
      "inputCursorCoordinate": {
        "lineIndex": 0,
        "charIndex": 0
      },
      "expectedIndex": 0
    }
  ]
}
