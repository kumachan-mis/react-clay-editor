{
  "name": "react-clay-editor",
  "version": "1.0.0",
  "description": "A document editor quick editable and flexible like clay in hands",
  "keywords": [
    "react",
    "editor"
  ],
  "homepage": "https://kumachan-mis.github.io/react-clay-editor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumachan-mis/react-clay-editor.git"
  },
  "license": "MIT",
  "author": "Yuya Suwa (@kumachan-mis)",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/lib/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn clean && vite --config viteconfig.lib.ts build && tsc --build tsconfig.lib.json",
    "build-docs": "build-storybook --docs",
    "clean": "rm -Rf dist",
    "deploy-docs": "yarn build-docs && gh-pages -d storybook-static",
    "dev-docs": "start-storybook -p 6006 --docs --no-manager-cache",
    "dev-test": "vite --config viteconfig.tests.ts",
    "format": "sort-package-json && prettier --write \"**/*.{css,htm,html,js,jsx,json,md,mdx,scss,ts,tsx,yaml,yml}\"",
    "gen-icons": "svgr --typescript --out-dir icons/dist -- icons",
    "lint": "eslint \"{src,docs,target,tests,unittests}/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "yarn build",
    "test": "playwright test",
    "typecheck": "yarn tsc --noEmit",
    "unittest": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@jest/globals": "^29.3.1",
    "@mdx-js/react": "^1.6.22",
    "@playwright/test": "^1.29.1",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-docs": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addons": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/theming": "^6.5.15",
    "@svgr/cli": "^6.5.1",
    "@types/katex": "^0.16.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "katex": "^0.16.4",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sort-package-json": "^2.1.0",
    "ts-jest": "^29.0.3",
    "typescript": "4.8.4",
    "vite": "^4.0.3"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.0.0",
    "@emotion/styled": ">=11.0.0",
    "katex": ">=0.16.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "packageManager": "yarn@3.2.4"
}
