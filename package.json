{
  "name": "react-realtime-markup-editor",
  "homepage": "https://kumachan-mis.github.io/react-realtime-markup-editor",
  "version": "0.13.4",
  "description": "A text document editor which is syntactically formattable in real time",
  "keywords": [
    "react",
    "document",
    "editor",
    "real time",
    "syntactic"
  ],
  "files": [
    "dist/**/*",
    "figures/**/*",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumachan-mis/react-realtime-markup-editor.git"
  },
  "author": "Yuya Suwa (@kumachan-mis)",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "katex": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/katex": "^0.11.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.2.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx",
      "jsx"
    ],
    "setupFilesAfterEnv": [
      "./tests/jest.setup.ts"
    ]
  },
  "scripts": {
    "build": "tsc && cp src/style.css dist/style.css",
    "build-sample": "",
    "start": "rm -Rf sample/dist && webpack serve --config webpack.config.dev.js",
    "test": "jest",
    "typecheck": "tsc --build tsconfig.json",
    "lint": "eslint \"{src,tests,sample}/**/*.{ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prettier": "prettier --write \"**/*.{css,html,json,scss,ts,tsx,}\"",
    "prepublishOnly": "yarn build",
    "predeploy": "yarn build-sample --public-url https://kumachan-mis.github.io/react-realtime-markup-editor",
    "deploy": "gh-pages -d sample/dist"
  }
}
