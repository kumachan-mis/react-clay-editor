{
  "name": "react-realtime-markup-editor",
  "homepage": "https://kumachan-mis.github.io/react-realtime-markup-editor",
  "version": "0.20.0",
  "description": "A text document editor which is syntactically formattable in real time",
  "keywords": [
    "react",
    "document",
    "editor",
    "real time",
    "syntactic"
  ],
  "files": [
    "dist/**/*",
    "figures/**/*",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumachan-mis/react-realtime-markup-editor.git"
  },
  "author": "Yuya Suwa (@kumachan-mis)",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.2.1",
    "@types/katex": "0.11.1",
    "@types/react": "17.0.27",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-loader": "8.2.2",
    "cpx": "1.5.0",
    "css-loader": "6.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "gh-pages": "3.2.3",
    "html-webpack-plugin": "5.3.2",
    "husky": "7.0.2",
    "jest": "27.2.4",
    "katex": "0.13.18",
    "lint-staged": "11.2.0",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.42.1",
    "style-loader": "3.3.0",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3",
    "webpack": "5.58.0",
    "webpack-cli": "4.9.0",
    "webpack-dev-server": "4.3.1",
    "webpack-merge": "5.8.0"
  },
  "peerDependencies": {
    "katex": ">=0.13.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "scripts": {
    "build": "rm -Rf dist && yarn gen-css && tsc --build tsconfig.json && cpx \"src/**/*.css\" dist",
    "build-sample": "rm -Rf sample/dist && yarn gen-css && webpack --config webpack.config.prod.js",
    "clean": "rm -Rf dist sample/dist {src,sample}/**/*.css",
    "deploy": "gh-pages -d sample/dist",
    "gen-css": "rm -Rf {src,sample}/**/*.css && sass src:src sample:sample --no-source-map",
    "lint": "eslint \"{src,tests,sample}/**/*.{ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "predeploy": "yarn build-sample",
    "prepublishOnly": "yarn build",
    "format": "prettier --write \"**/*.{css,html,json,scss,ts,tsx}\"",
    "start": "rm -Rf sample/dist && yarn gen-css && webpack serve --config webpack.config.dev.js",
    "test": "jest",
    "typecheck": "yarn tsc --noEmit"
  }
}
