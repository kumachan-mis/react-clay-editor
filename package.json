{
  "name": "react-clay-editor",
  "version": "1.1.0",
  "description": "A document editor quick editable and flexible like clay in hands",
  "keywords": [
    "react",
    "editor"
  ],
  "homepage": "https://kumachan-mis.github.io/react-clay-editor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumachan-mis/react-clay-editor.git"
  },
  "license": "MIT",
  "author": "Yuya Suwa (@kumachan-mis)",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/lib/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn clean && vite --config viteconfig.lib.ts build && tsc --build tsconfig.lib.json",
    "build-storybook": "storybook build --docs",
    "clean": "rm -Rf dist",
    "deploy-storybook": "yarn build-storybook && gh-pages -d storybook-static",
    "dev": "vite --config viteconfig.tests.ts",
    "format": "sort-package-json && prettier --write \"**/*.{css,htm,html,js,jsx,json,md,mdx,scss,ts,tsx,yaml,yml}\"",
    "gen-icons": "svgr --typescript --out-dir icons/dist -- icons",
    "lint": "eslint \"{src,docs,target,tests,unittests}/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "yarn build",
    "preview-storybook": "vite preview --port 6006 --outDir storybook-static/",
    "storybook": "storybook dev --port 6006 --docs",
    "test": "playwright test",
    "typecheck": "yarn tsc --noEmit",
    "unittest": "jest"
  },
  "devDependencies": {
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@jest/globals": "^29.7.0",
    "@mdx-js/react": "^3.0.0",
    "@playwright/test": "^1.39.0",
    "@storybook/addon-actions": "^7.5.3",
    "@storybook/addon-docs": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/builder-vite": "^7.5.3",
    "@storybook/channels": "^7.5.3",
    "@storybook/global": "^5.0.0",
    "@storybook/manager-api": "^7.5.3",
    "@storybook/preview-api": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/theming": "^7.5.3",
    "@svgr/cli": "^8.1.0",
    "@types/katex": "^0.16.6",
    "@types/node": "^20.9.0",
    "@types/prettier": "2.7.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "katex": "^0.16.9",
    "lint-staged": "^15.0.2",
    "prettier": "2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-gfm": "3.0.1",
    "sort-package-json": "^2.6.0",
    "storybook": "^7.5.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.0.0",
    "@emotion/styled": ">=11.0.0",
    "katex": ">=0.16.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "packageManager": "yarn@4.0.1"
}
