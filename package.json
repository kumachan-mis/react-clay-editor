{
  "name": "react-realtime-markup-editor",
  "homepage": "https://kumachan-mis.github.io/react-realtime-markup-editor",
  "version": "0.16.1",
  "description": "A text document editor which is syntactically formattable in real time",
  "keywords": [
    "react",
    "document",
    "editor",
    "real time",
    "syntactic"
  ],
  "files": [
    "dist/**/*",
    "figures/**/*",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumachan-mis/react-realtime-markup-editor.git"
  },
  "author": "Yuya Suwa (@kumachan-mis)",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/katex": "^0.11.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-loader": "^8.2.2",
    "cpx": "^1.5.0",
    "css-loader": "^5.2.5",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "katex": "^0.13.11",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "peerDependencies": {
    "katex": ">=0.13.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "scripts": {
    "build": "rm -Rf dist && tsc --build tsconfig.json && cpx \"src/**/*.css\" dist",
    "build-sample": "rm -Rf sample/dist && webpack --config webpack.config.prod.js",
    "deploy": "gh-pages -d sample/dist",
    "lint": "eslint \"{src,tests,sample}/**/*.{ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "predeploy": "yarn build-sample",
    "prepublishOnly": "yarn build",
    "prettier": "prettier --write \"**/*.{css,html,json,scss,ts,tsx,}\"",
    "start": "rm -Rf sample/dist && webpack serve --config webpack.config.dev.js",
    "test": "jest",
    "typecheck": "yarn tsc --noEmit"
  }
}
