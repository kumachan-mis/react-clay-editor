{
  "name": "react-realtime-markup-editor",
  "homepage": "https://kumachan-mis.github.io/react-realtime-markup-editor",
  "version": "0.23.0",
  "description": "A text document editor which is syntactically formattable in real time",
  "keywords": [
    "react",
    "document",
    "editor",
    "real time",
    "syntactic"
  ],
  "files": [
    "dist/**/*",
    "figures/**/*",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumachan-mis/react-realtime-markup-editor.git"
  },
  "author": "Yuya Suwa (@kumachan-mis)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@testing-library/dom": "8.7.2",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.4.2",
    "@types/jest": "27.0.2",
    "@types/katex": "0.11.1",
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.10",
    "@types/testing-library__jest-dom": "5.14.1",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "babel-loader": "8.2.3",
    "cpx": "1.5.0",
    "css-loader": "6.5.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "gh-pages": "3.2.3",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "katex": "0.13.20",
    "lint-staged": "11.2.6",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "style-loader": "3.3.1",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4",
    "webpack": "5.60.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.4.0",
    "webpack-merge": "5.8.0"
  },
  "peerDependencies": {
    "katex": ">=0.13.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "scripts": {
    "build": "rm -Rf dist && tsc --build tsconfig.build.json && cpx \"src/**/*.css\" dist",
    "build-sample": "rm -Rf sample/dist && webpack --config webpack.config.prod.js",
    "clean": "rm -Rf dist sample/dist ",
    "deploy": "yarn build-sample && gh-pages -d sample/dist",
    "lint": "eslint \"{src,tests,sample}/**/*.{ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "yarn build",
    "format": "prettier --write \"**/*.{css,html,json,scss,ts,tsx}\"",
    "start": "rm -Rf sample/dist && webpack serve --config webpack.config.dev.js",
    "test": "jest",
    "typecheck": "yarn tsc --noEmit"
  },
  "packageManager": "yarn@3.0.2"
}
