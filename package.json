{
  "name": "react-realtime-markup-editor",
  "version": "0.29.0",
  "description": "A text document editor which is syntactically formattable in real time",
  "keywords": [
    "react",
    "document",
    "editor",
    "real time",
    "syntactic"
  ],
  "homepage": "https://kumachan-mis.github.io/react-realtime-markup-editor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumachan-mis/react-realtime-markup-editor.git"
  },
  "license": "MIT",
  "author": "Yuya Suwa (@kumachan-mis)",
  "main": "dist/lib/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist/**/*",
    "figures/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn clean && yarn build-cjs && yarn build-esm",
    "build-cjs": "yarn clean-cjs && tsc --build tsconfig.cjs.json && cpx \"src/**/*.css\" dist/lib",
    "build-esm": "yarn clean-esm && tsc --build tsconfig.esm.json && cpx \"src/**/*.css\" dist/esm",
    "build-sample": "yarn clean-sample && cross-env ENVIRONMENT=production webpack",
    "clean": "rm -Rf dist",
    "clean-cjs": "rm -Rf dist/lib",
    "clean-esm": "rm -Rf dist/esm",
    "clean-sample": "rm -Rf sample/dist",
    "deploy": "yarn build-sample && gh-pages -d sample/dist",
    "format": "sort-package-json && yarn prettierify",
    "gen-icons": "svgr --typescript --out-dir icons/dist -- icons",
    "lint": "eslint \"{src,tests,sample}/**/*.{ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "yarn build",
    "prettierify": "prettier --write \"**/*.{css,html,json,scss,ts,tsx}\"",
    "start": "yarn clean-sample && cross-env ENVIRONMENT=development webpack serve",
    "test": "cross-env ENVIRONMENT=test jest",
    "typecheck": "yarn tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/preset-env": "7.19.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@emotion/css": "^11.10.5",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@svgr/cli": "6.5.1",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.4.2",
    "@types/jest": "29.2.0",
    "@types/katex": "0.14.0",
    "@types/node": "18.11.7",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "babel-loader": "9.0.0",
    "cpx": "1.5.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "gh-pages": "4.0.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "jest": "29.2.2",
    "jest-environment-jsdom": "29.2.2",
    "katex": "0.16.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sort-package-json": "2.0.0",
    "style-loader": "3.3.1",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.0.0",
    "@emotion/styled": ">=11.0.0",
    "katex": ">=0.16.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "packageManager": "yarn@3.2.4"
}
