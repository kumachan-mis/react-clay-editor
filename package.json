{
  "name": "react-realtime-markup-editor",
  "version": "^0.29.0",
  "description": "A text document editor which is syntactically formattable in real time",
  "keywords": [
    "react",
    "document",
    "editor",
    "real time",
    "syntactic"
  ],
  "homepage": "https://kumachan-mis.github.io/react-realtime-markup-editor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumachan-mis/react-realtime-markup-editor.git"
  },
  "license": "MIT",
  "author": "Yuya Suwa (@kumachan-mis)",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/lib/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "figures/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn clean && vite --config viteconfig.lib.ts build && tsc --build tsconfig.lib.json",
    "clean": "rm -Rf dist",
    "format": "sort-package-json && yarn prettierify",
    "gen-icons": "svgr --typescript --out-dir icons/dist -- icons",
    "lint": "eslint \"{src,target,tests,unittests}/**/*.{ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "yarn build",
    "prettierify": "prettier --write \"**/*.{css,html,json,scss,ts,tsx}\"",
    "test": "playwright test",
    "testdev": "vite --config viteconfig.tests.ts",
    "typecheck": "yarn tsc --noEmit",
    "unittest": "jest"
  },
  "devDependencies": {
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@jest/globals": "^29.3.1",
    "@playwright/test": "^1.29.1",
    "@svgr/cli": "^6.5.1",
    "@types/katex": "^0.16.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "katex": "^0.16.4",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sort-package-json": "^2.1.0",
    "typescript": "4.8.4",
    "vite": "^4.0.3"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.0.0",
    "@emotion/styled": ">=11.0.0",
    "katex": ">=0.16.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "packageManager": "yarn@3.2.4"
}
