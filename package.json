{
  "name": "react-realtime-markup-editor",
  "version": "0.29.0",
  "description": "A text document editor which is syntactically formattable in real time",
  "keywords": [
    "react",
    "document",
    "editor",
    "real time",
    "syntactic"
  ],
  "homepage": "https://kumachan-mis.github.io/react-realtime-markup-editor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumachan-mis/react-realtime-markup-editor.git"
  },
  "license": "MIT",
  "author": "Yuya Suwa (@kumachan-mis)",
  "main": "dist/lib/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist/**/*",
    "figures/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn clean && yarn build-cjs && yarn build-esm",
    "build-cjs": "yarn clean-cjs && exit 1",
    "build-esm": "yarn clean-esm && exit 1",
    "build-sample": "yarn clean-sample && exit 1",
    "build-target": "yarn clean-target && exit 1",
    "clean": "rm -Rf dist",
    "clean-cjs": "rm -Rf dist/lib",
    "clean-esm": "rm -Rf dist/esm",
    "clean-sample": "rm -Rf sample/dist",
    "clean-target": "rm -Rf tests/dist",
    "deploy": "yarn build-sample && gh-pages -d sample/dist",
    "format": "sort-package-json && yarn prettierify",
    "gen-icons": "svgr --typescript --out-dir icons/dist -- icons",
    "lint": "eslint \"{src,sample,target,tests,unittests}/**/*.{ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "yarn build",
    "prettierify": "prettier --write \"**/*.{css,html,json,scss,ts,tsx}\"",
    "start-sample": "yarn clean-sample && cross-env ENVIRONMENT=development exit 1",
    "start-target": "yarn clean-target && cross-env ENVIRONMENT=test  exit 1",
    "test": "yarn build-target && playwright test",
    "typecheck": "yarn tsc --noEmit",
    "unittest": "cross-env ENVIRONMENT=test jest"
  },
  "devDependencies": {
    "@emotion/css": "11.10.5",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@jest/globals": "29.3.1",
    "@playwright/test": "1.28.1",
    "@svgr/cli": "6.5.1",
    "@types/katex": "0.14.0",
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "cross-env": "7.0.3",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "gh-pages": "4.0.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "katex": "0.16.3",
    "lint-staged": "13.0.4",
    "prettier": "2.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sort-package-json": "2.1.0",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.0.0",
    "@emotion/styled": ">=11.0.0",
    "katex": ">=0.16.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "packageManager": "yarn@3.2.4"
}
