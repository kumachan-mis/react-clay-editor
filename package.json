{
  "name": "react-realtime-markup-editor",
  "homepage": "https://kumachan-mis.github.io/react-realtime-markup-editor",
  "version": "0.13.3",
  "description": "A text document editor which is syntactically formattable in real time",
  "keywords": [
    "react",
    "document",
    "editor",
    "real time",
    "syntactic"
  ],
  "files": [
    "dist/**/*",
    "figures/**/*",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumachan-mis/react-realtime-markup-editor.git"
  },
  "author": "Yuya Suwa (@kumachan-mis)",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "katex": "^0.12.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.1",
    "@types/katex": "^0.11.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "parcel-bundler": "1.12.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx",
      "jsx"
    ],
    "setupFilesAfterEnv": [
      "./tests/jest.setup.ts"
    ]
  },
  "scripts": {
    "build": "tsc && cp src/style.css dist/style.css",
    "start": "rm -Rf sample/dist && parcel sample/src/index.html --out-dir sample/dist --open",
    "test": "jest",
    "typecheck": "tsc --build tsconfig.json",
    "lint": "eslint \"{src,tests,sample}/**/*.{ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prettier": "prettier --write \"**/*.{css,html,json,scss,ts,tsx,}\"",
    "prepublishOnly": "yarn build",
    "predeploy": "rm -Rf sample/dist && parcel build sample/src/index.html --out-dir sample/dist --public-url https://kumachan-mis.github.io/react-realtime-markup-editor",
    "deploy": "gh-pages -d sample/dist"
  }
}
