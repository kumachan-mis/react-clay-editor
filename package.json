{
  "name": "react-realtime-markup-editor",
  "version": "0.24.0",
  "description": "A text document editor which is syntactically formattable in real time",
  "keywords": [
    "react",
    "document",
    "editor",
    "real time",
    "syntactic"
  ],
  "homepage": "https://kumachan-mis.github.io/react-realtime-markup-editor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumachan-mis/react-realtime-markup-editor.git"
  },
  "license": "MIT",
  "author": "Yuya Suwa (@kumachan-mis)",
  "main": "dist/lib/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist/**/*",
    "figures/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn clean && yarn build-cjs && yarn build-esm",
    "build-cjs": "yarn clean-cjs && tsc --build tsconfig.cjs.json && cpx \"src/**/*.css\" dist/lib",
    "build-esm": "yarn clean-esm && tsc --build tsconfig.esm.json && cpx \"src/**/*.css\" dist/esm",
    "build-sample": "yarn clean-sample && cross-env ENVIRONMENT=production webpack",
    "clean": "rm -Rf dist",
    "clean-cjs": "rm -Rf dist/lib",
    "clean-esm": "rm -Rf dist/esm",
    "clean-sample": "rm -Rf sample/dist",
    "deploy": "yarn build-sample && gh-pages -d sample/dist",
    "format": "sort-package-json && yarn prettierify",
    "lint": "eslint \"{src,tests,sample}/**/*.{ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "yarn build",
    "prettierify": "prettier --write \"**/*.{css,html,json,scss,ts,tsx}\"",
    "start": "yarn clean-sample && cross-env ENVIRONMENT=development webpack serve",
    "test": "cross-env ENVIRONMENT=test jest",
    "typecheck": "yarn tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@testing-library/dom": "8.11.0",
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.0.2",
    "@types/katex": "0.11.1",
    "@types/node": "16.11.6",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "@types/testing-library__jest-dom": "5.14.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "babel-loader": "8.2.3",
    "cpx": "1.5.0",
    "cross-env": "7.0.3",
    "css-loader": "6.5.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "gh-pages": "3.2.3",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "katex": "0.15.1",
    "lint-staged": "11.2.6",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sort-package-json": "1.52.0",
    "style-loader": "3.3.1",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4",
    "webpack": "5.62.1",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.4.0"
  },
  "peerDependencies": {
    "katex": ">=0.13.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "packageManager": "yarn@3.0.2"
}
