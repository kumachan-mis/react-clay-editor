{
  "name": "react-clay-editor",
  "version": "1.1.0",
  "description": "A document editor quick editable and flexible like clay in hands",
  "keywords": [
    "react",
    "editor"
  ],
  "homepage": "https://kumachan-mis.github.io/react-clay-editor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumachan-mis/react-clay-editor.git"
  },
  "license": "MIT",
  "author": "Yuya Suwa (@kumachan-mis)",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/lib/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn clean && vite --config viteconfig.lib.ts build && tsc --build tsconfig.lib.json",
    "build-docs": "storybook build --docs",
    "clean": "rm -Rf dist",
    "deploy-docs": "yarn build-docs && gh-pages -d storybook-static",
    "dev": "vite --config viteconfig.tests.ts",
    "dev-docs": "storybook dev -p 6006 --docs",
    "format": "sort-package-json && prettier --write \"**/*.{css,htm,html,js,jsx,json,md,mdx,scss,ts,tsx,yaml,yml}\"",
    "gen-icons": "svgr --typescript --out-dir icons/dist -- icons",
    "lint": "eslint \"{src,docs,target,tests,unittests}/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "yarn build",
    "test": "playwright test",
    "typecheck": "yarn tsc --noEmit",
    "unittest": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@emotion/css": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@jest/globals": "^29.5.0",
    "@mdx-js/react": "^2.3.0",
    "@playwright/test": "^1.34.3",
    "@storybook/addon-actions": "^7.0.18",
    "@storybook/addon-docs": "^7.0.18",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/channel-postmessage": "^7.0.18",
    "@storybook/channel-websocket": "^7.0.18",
    "@storybook/manager-api": "^7.0.18",
    "@storybook/preview-api": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/react-webpack5": "^7.0.18",
    "@storybook/theming": "^7.0.18",
    "@svgr/cli": "^8.0.1",
    "@types/katex": "^0.16.0",
    "@types/node": "^20.2.5",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "katex": "^0.16.7",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-gfm": "^3.0.1",
    "sort-package-json": "^2.4.1",
    "storybook": "^7.0.18",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.9"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.0.0",
    "@emotion/styled": ">=11.0.0",
    "katex": ">=0.16.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "packageManager": "yarn@3.5.1"
}
