{
  "name": "react-clay-editor",
  "version": "0.0.0-semantically-released",
  "description": "A document editor quick editable and flexible like clay in hands",
  "keywords": [
    "react",
    "editor"
  ],
  "homepage": "https://kumachan-mis.github.io/react-clay-editor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumachan-mis/react-clay-editor.git"
  },
  "license": "MIT",
  "author": "Yuya Suwa (@kumachan-mis)",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.umd.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build-app": "yarn clean-app && vite --config viteconfig.app.mts build",
    "build-lib": "yarn clean-lib && vite --config viteconfig.lib.mts build && tsc --build tsconfig.lib.json",
    "build-storybook": "yarn clean-storybook && storybook build --docs && touch ./storybook-static/.nojekyll",
    "clean-app": "rm -Rf app/dist",
    "clean-lib": "rm -Rf dist",
    "clean-storybook": "rm -rf node_modules/ storybook-static/",
    "dev": "vite --config viteconfig.app.mts",
    "format": "sort-package-json && prettier --write \"**/*.{css,htm,html,js,jsx,json,md,mdx,scss,ts,tsx,yaml,yml}\"",
    "gen-icons": "svgr --typescript --out-dir icons/dist -- icons",
    "lint": "eslint \"{src,docs,app,tests,unittests}/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "yarn build-lib",
    "preview-app": "vite --config viteconfig.app.mts preview",
    "preview-storybook": "vite preview --port 6006 --outDir storybook-static/",
    "storybook": "storybook dev --port 6006 --docs",
    "test": "playwright test",
    "typecheck": "yarn tsc --noEmit",
    "unittest": "jest"
  },
  "devDependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@jest/globals": "^29.7.0",
    "@mdx-js/react": "^3.1.0",
    "@playwright/test": "^1.50.1",
    "@storybook/addon-docs": "^8.5.8",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/addon-links": "^8.5.8",
    "@storybook/builder-vite": "^8.5.8",
    "@storybook/manager-api": "^8.5.8",
    "@storybook/react": "^8.5.8",
    "@storybook/react-vite": "^8.5.8",
    "@storybook/theming": "^8.5.8",
    "@svgr/cli": "^8.1.0",
    "@types/katex": "^0.16.7",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "katex": "^0.16.21",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "remark-gfm": "^4.0.1",
    "sort-package-json": "^2.14.0",
    "storybook": "^8.5.8",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3",
    "vite": "^6.1.1"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.0.0",
    "@emotion/styled": ">=11.0.0",
    "katex": ">=0.16.0",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "packageManager": "yarn@4.5.3"
}
